  <main class="text-center" style="max-width: 425px; margin: 0 auto; margin-top: 120px;">
  <% flash.each do |key, value| %>
    <div class="notice">
      <div class="alert alert-warning alert-dismissible fade show flash <%= key %>" role="alert">
        <%= value %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
    </div>
  <% end %>
  <h2 class="mb-4">Login Usu√°rios</h2>
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form-floating">
      <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"form-control" %>
      <%= f.label :email %>
    </div>
    <div class="form-floating">
      <%= f.password_field :password, autocomplete: "current-password", class:"form-control" %>
      <%= f.label :password %>
    </div>
    <% if devise_mapping.rememberable? %>
      <div class="form-check text-start my-3">
        <%= f.check_box :remember_me, class:'form-check-input' %>
        <%= f.label :remember_me, class: '"form-check-label"' %>
      </div>
    <% end %>
    <div class="actions my-4">
      <%= f.submit "Log in", class:"btn btn-primary w-100 py-2" %>
    </div>
  <% end %>
  <%= render "admins/shared/links" %>
</main>
